package com.numberprograms.com;

public class Arms2 {

	public static void main(String[] args) {
		int n = 0, rev = 0, rem = 0;

		for (int i = 1; i <= 1000; i++) {
			n = i;
			rem = n % 10;
			rev = rev + (rem * rem * rem);
			n = n / 10;
		}
		if (n == rev) {
			System.out.println(n);
		}

	}

}
