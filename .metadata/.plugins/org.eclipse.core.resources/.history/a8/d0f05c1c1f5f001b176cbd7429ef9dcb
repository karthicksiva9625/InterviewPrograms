package com.numberprograms.com;

public class Arms2 {

	public static void main(String[] args) {
		for (int i = 0; i <= 1000; i++) {
			int a = 0, j = 0, n = 0;
			a = n;
			if (a > 0) {
				i = a % 10;
				j = j + (i * i * i);
				a = a / 10;
			}
			if (n == j) {
				System.out.println(n);
			}

		}

	}

}
