package com.numberprograms.com;

public class Arms2 {

	public static void main(String[] args) {

		int rem = 0, rev = 0, a;
		for (int i = 1; i <= 1000; i++) {
			a = i;
			while (a > 0) {
				rem = a % 10;
				rev = rev + (rem * rem * rem);
				a = a / 10;
			}
			if (i == rev) {
				System.out.println(a);
			}

		}
	}
}
