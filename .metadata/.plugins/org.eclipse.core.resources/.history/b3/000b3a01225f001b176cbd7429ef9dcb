package com.numberprograms.com;

public class Arms2 {

	public static void main(String[] args) {

		for (int i = 1; i <= 1000; i++) {

			int a, rem = 0, rev = 0;
			a = i;
			while (a > 0) {
				rem = a % 10;
				rev = rev + (rem * rem * rem);
				a = rem / 10;
			}

			if (a == rev) {
				System.out.println(a);

			}

		}

	}
}
